{
  "manifest_version": 3,
  "name": "My-Commute",
  "version": "1.0.0",
  "description": "Adds google Maps link on Finn.no real estate pages.",
  "permissions": ["storage"],
  "icons": {
    "16": "icons/mycommute_16.png",
    "32": "icons/mycommute_32.png",
    "48": "icons/mycommute_48.png",
    "128": "icons/mycommute_128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.finn.no/realestate/homes/ad.html?finnkode=*"],
      "js": ["app.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Finn.no reisetid innstillinger"
  }
}